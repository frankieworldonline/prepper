<modpreppers>
	<!-- Preppers Mod  --> 
	
	<append xpath="/blocks">

<!-- prepper class bags -->

	<block name="SmackToOpenBag">
		<property name="CustomIcon" value="cntBackpack03" />
	   <property name="Material" value="Mcloth"/>
		<property name="LightOpacity" value="0"/>
		<property name="StabilitySupport" value="false"/> <!-- build restriction -->
		<property name="Shape" value="Ext3dModel"/> <property name="Texture" value="293"/>
		<property name="Shape" value="ModelEntity"/>
		<property name="Model" value="Entities/LootContainers/backpack01Prefab" />
		<property name="IsTerrainDecoration" value="true"/>
		<property name="DowngradeBlock" value="PreppersBugOutBag" />
		<property name="Place" value="TowardsPlacer" />
		<drop event="Destroy" count="0" />
	</block>
	<block name="PreppersBugOutBag">
		<property name="Extends" value="SmackToOpenBag" param1="DowngradeBlock" />
		<property name="Class" value="Loot" />
